@using KnowledgeBase.Logic.Dto;
@model KnowledgeBase.Logic.Dto.AddTagDto;


@using (Html.BeginForm("AddTags", "Project",new { id = ViewBag.ProjectId } , FormMethod.Post, true, new {@class = "form"}))
{
    <div>Insert tags separated by comma</div>
    @Html.TextBoxFor(tags => tags.Tags, new {@class = "form-control"})    
    <input type="Submit" value="Add" class="btn btn-primary"/>
}
