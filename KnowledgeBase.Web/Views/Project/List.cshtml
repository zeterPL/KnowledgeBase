@using KnowledgeBase.Logic.Dto.Project;
@model IEnumerable<ProjectDto>
@{
    IEnumerable<ProjectDto> projects = Model;
}

@foreach (ProjectDto project in projects)
{
    <div>
        @project.Id
        @project.Name
        <a asp-action="Details" asp-route-id="@project.Id">details</a>
        <a asp-action="Edit" asp-route-id="@project.Id">edit</a>
        <a asp-action="Delete" asp-route-id="@project.Id">delete</a>
        <a asp-action="AssignUsers" asp-route-id="@project.Id">Assign users</a>
        <a asp-action="ReportsList" asp-route-id="@project.Id">Reported issues</a>
    </div>
}